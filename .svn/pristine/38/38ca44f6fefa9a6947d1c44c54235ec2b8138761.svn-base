html, body, div, span, object, iframe,
h1, h2, h3, h4, h5, h6, p, blockquote, pre,
a, abbr, acronym, address,
del, dfn, em, img, ins, q,
small, strong, sub, sup,
dl, dt, dd, ol, ul, li,
fieldset, form, label, legend {
	margin: 0;
	padding: 0;
	border: 0;
	outline: 0;
	font-size: 100%;
	vertical-align: baseline;
	background: transparent;
}
ol, ul {
	list-style: disc;
}
a:focus,
input:focus {
	outline: 1px dotted #fc0;
}



body {
	background-color: #000000;
	font-family: "Lucida Grande",Arial,Helvetica,sans-serif; 
	color: #666;
	font-size: 11px;
	line-height: 1;
}
#top_bar_container,
header nav,
footer nav,
#uberfooter,
#alpha_links li,
#index_modules h2,
#uberfooter h2,
#searchform #searchterm,
footer {
	font-family: Gotham, "Gotham SSm A", "Gotham SSm B", Muli, sans-serif;
	text-rendering: optimizeLegibility;
}
a {
	text-decoration: none;
	color: #fff;
}
a:hover {
	text-decoration: underline;
	color: #fc0;
}
br.clear {
	clear: both;
	height: 1px;
	overflow: hidden;
}
.mask, .sr-only {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  overflow: hidden;
  clip: rect(0,0,0,0);
  white-space: nowrap;
  clip-path: inset(50%);
  border: 0;
}
#top_bar_container,
header nav,
footer nav {
	font-size: 9.5px;
	font-weight: 400;
	text-transform: uppercase;
}
header nav {
	letter-spacing: -0.1px;
}
header nav,
footer nav ul ul {
	font-weight: 500;
}
#top_bar_container .inlineimglinks a,
header nav .inlineimglinks a,
footer nav .inlineimglinks a {
	display: inline-block;
}
#top_bar_container .inlineimglinks a {
	margin-left: 2em;
}
#top_bar_container {
	background-color: #002119;
	background-image: url(https://static.ndsu.nodak.edu/templates/images/bg_header.jpg);
	background-repeat: no-repeat;
	background-position: top left;
}
#top_bar {
	position: relative;
	margin: 0 auto;
	width: 766px;
	color: #fff;
	/*line-height: 16px;*/
}
#top_bar a {
	color: #fc0;
}
#top_bar a:hover {
	color: #fff;
}
#top_bar h1, #top_bar ul {
	vertical-align: middle;
	display: inline-block;
}
#top_bar ul{
	position: absolute;
	right: 0;
	bottom: 0;
	top: 14px;
}
header nav {
	background-color: #fff;
	min-height: 20px;
	line-height: 20px;
}
header nav a {
	color: #000;
}
header nav a:hover {
	color: #666;
}
header nav ul {
	color: #000;
}
header nav > ul {
	text-align: justify;
	width: 766px;
	height: 1em;
}
header nav > ul:after {
	content: "";
	display: inline-block;
	width: 100%;
}
header nav ul ul,
footer nav ul ul {
	padding: 10px;
	line-height: 13px;
	background: #fff;
	background: rgba(255,255,255,0.9);
	text-transform: none;
	font-size: 10px;
	text-align: left;
}
footer nav ul ul li {
	margin: 0;
}
footer nav ul ul a {
	color: #000;
	text-indent: -1em;
	padding-left: 1em;
}
footer nav {
	background-image: url(https://static.ndsu.nodak.edu/templates/images/bg_footernav.png);
	background-repeat: repeat-x;
	line-height: 24px;
	text-align: center;
}
footer nav ul li {
	margin: 0 12px;
}
footer {
	margin-top: 15px;
}
