#uberfooter {
	width: 766px;
	margin: 0 auto;
	font-size: 10px;
	font-weight: 300;
	padding-bottom: 30px;
	padding-top: 2em;
	clear: both;
}
#uberfooter ul {
	display: block;
	width: 19%;
	margin: 0;
	padding: 0 1% 0 0;
	float: left;
	line-height: 1em;
}
#uberfooter ul li.heading {
	text-transform: uppercase;
	font-size: 1.25em;
}
#uberfooter ul li {
	margin: 0 0 0 1em;
	text-indent: -1em;
	padding: 0;
	display: block;
	float: none;
	line-height: 1.5em;
	color: #fc0;
	height: auto !important;
}
#uberfooter ul li a {
	margin: 0;
	padding: 0;
	display: inline;
	color: #fff;
	text-decoration: none;
}
#uberfooter ul li a:hover {
	color: #fc0;
	text-decoration: underline;
}

main {
	margin: 0px auto;
	width: 767px;
	padding: 33px 0px 9px 0px;
}
#logo_ndsu_fargo {
	margin-bottom: 6px;
	height: 41px;
}
#logo_ndsu_fargo h2 {
	color: #fff;
	float: left;
	width: 45%;
}
#logo_ndsu_fargo p {
	float: right;
	width: 45%;
	text-align: right;
}
#index_modules {
	padding: 22px 0px 0px 0px;
}
#sflgru {
	text-align: center;
	clear: both;
	background: #002119 url(https://static.ndsu.nodak.edu/templates/images/sflgru_bar.png) no-repeat center center;
	height: 29px;
	margin-top: 1em;/* home page only */
}
#sflgru p {
	padding: 0;
	margin: 0;
	visibility: hidden;
}
#alpha_links {
	padding-top: 9px;
	font-weight: 600;
	height: 25px;
}
#alpha_links ul {
	list-style: none;
	padding: 0;
	margin: 0;
}
#alpha_links a {
	display: block;
	height: 22px;
	line-height: 22px;
	text-align: center;
	width: 100%;
	float: left;
	font-size: 10px;
	color: #fff;
	background-color: #333;
	border: 2px solid black;
}
#alpha_links a:hover {
	color: #333;
	background-color: #aaa;
}
#alpha_links li {
	font-weight: 600;
	width: 23px;
	height: 22px;
	float: left;
}
#alpha_links li.index {
	width: 162px;
}
#alpha_links li.index a {
	background-color: #fc0;
	text-transform: uppercase;
	color: #333;
	font-size: 9px;
	line-height: 23px;
}
#index_modules h2,
#uberfooter h2 {
	padding-bottom: 10px;
	text-transform: uppercase;
	font-weight: 600;
	color: #fc0;
	font-size: 11.5px;
}
#uberfooter h2 {
	padding-bottom: 0;
	line-height: 15px;
}
#index_modules h2 a {
	color: #fc0;
}
#index_modules h2 a:hover {
	color: #fff;
}
#index_modules > div {
	padding: 9px 8px 9px 8px;
	margin-right: 8px;
	height: 120px;
	border: 1px solid #404041;
	float: left;
}
#module_left {
	width: 363px;
}
#module_middle {
	width: 163px;
}
#module_right {
	float: right;
	margin-right: 0px !important;
	width: 169px;
}
a:hover .imghover {
	opacity: 0.5;
}
.antiimghover {
	opacity: 0.5;
}
a:hover .antiimghover {
	opacity: 1;
}
.inlineimglinks {
	margin: 0 auto;
	list-style: none;
}
.inlineimglinks li {
	display: inline;
}
#searchform {
	margin-bottom: 2em;
	border: 0;
}
#searchform input {
	font-size: 1.3em;
	height: 2em;
	line-height: 2em;
	border: none;
	margin: 0;
	padding: 0;
}
#searchform #searchterm {
	background: #6d6e71;
	-moz-border-radius: 0.8em;
	-webkit-border-radius: 0.8em;
	-o-border-radius: 0.8em;
	-ms-border-radius: 0.8em;
	border-radius: 0.8em;
	width: 100%;
	color: #000;
	padding: 0 3.5em 0 0.5em;
	margin: 0;
	box-sizing: border-box;
	-webkit-appearance: none;
}
#searchform #searchterm:focus {
	background-color: #fff;
	outline: none;
}
#searchform #searchsubmit {
	position: relative;
	top: calc(-6px - 1.1em);
	top: -webkit-calc(-6px - 1.1em);
	float: right;
	height: 1.2em;
	padding: 0 0.5em;
	border-left: 2px ridge #333;
	-moz-border-radius: 0;
	-webkit-border-radius: 0;
	-o-border-radius: 0;
	-ms-border-radius: 0;
	border-radius: 0;
}
#searchform input:-ms-input-placeholder {
	color: #222;
}
#searchform input:focus:-ms-input-placeholder {
	color: #6d6e71;
}
#searchform input::-webkit-input-placeholder {
	color: #222;
}
#searchform input:focus::-webkit-input-placeholder {
	color: #6d6e71;
}
#searchform input::-moz-placeholder {
	color: #222;
}
#searchform input:focus::-moz-placeholder {
	color: #6d6e71;
}
.social-media span {
	position: absolute;
	top: -5000px;
	left: -5000px;
}
.social-media a:hover {
	opacity: 0.6;
}
.social-media a {
	height: 25px;
	width: 25px;
	background: url(https://static.ndsu.nodak.edu/templates/images/icon_social_media.png) no-repeat top left;
	display: inline-block;
}
.social-media .icon-twitter {
	background-position: -25px 0px;
}
.social-media .icon-instagram {
	background-position: -50px 0px;
}
.social-media .icon-youtube {
	background-position: -75px 0px;
}
.social-media .icon-rss {
	background-position: -100px 0px;
}
.social-media .icon-googlemap {
	background-position: -125px 0px;
}

#footernav {
	position: relative;
}
#footernav ul ul {
    bottom: 24px;
    width: 130px;        /* upped the width to match the image size */
    padding-bottom: 1em;    /* unrelated, but helps people from over-shooting the menu */
}
ul.dropdown {
	position: relative;
}

ul.dropdown li {

}

ul.dropdown li.hover,
ul.dropdown li:hover {
 position: relative;
 z-index: 599;
 cursor: default;
}

ul.dropdown ul {
 visibility: hidden;
 position: absolute;
 top: 100%;
 left: 0;
 z-index: 598;
}

ul.dropdown ul li {

}

ul.dropdown li:hover > ul {
 visibility: visible;
}
ul.dropdown-upward ul li {
	display: block;
}
ul.dropdown-upward ul {
 top: auto !important;
 bottom: 100%;
}
ul.inlineimglinks.dropdown-downward ul {
	top: 10px;
	bottom: auto !important;
}
@media screen and (-webkit-min-device-pixel-ratio:0) {
	ul.inlineimglinks.dropdown-downward ul {
	top: 16px;
	}
}
ul.inlineimglinks.dropdown-downward ul li {
	display: block;
}
#banner {
	height: 387px;
	width: 766px !important;
	overflow: hidden !important;
}
#banner ul {
	margin: 0px;
	padding: 0px;
	list-style: none;
	z-index: 100;
}
#banner li {
	z-index: 101;
	height: 387px;
	width: 766px;
	position: relative;
	z-index: 100;
}
#banner li img {
	position: absolute;
	top: 0px;
	left: 0px;
	z-index: 101;
}
#banner li span.banner_label {
	display: block;
	position: absolute;
	line-height: 15px;
	background-image: url(images/bg_banner.png);
	top: 340px;
	width: 766px;
	height: 47px;
	z-index: 102;
}
#banner li span.banner_label span {
	display: block;
	top: 0px;
	padding: 7px;
	background-image: none;
	color: #231F20;
	z-index: 103;
}
#banner .banner_label a {
	color: #f60;
}
#banner .banner_label a:hover {
	color: #231f20;
}
#slide_next span, #slide_prev span, #slide_pause span {
	display: none;
}
#slide_next a,
#slide_prev a,
#slide_pause a,
#slide_play a {
	position: absolute;
	display: block;
	height: 12px;
	width: 12px;
	background-image: url(file:///home/diparama/Projects/ndsutemplate/src/assets/images/banner_controls.png);
	background-repeat: no-repeat;
}

#slide_next a {
	top: 371px;
	left: 724px;
	z-index: 104;
}
#slide_next a:hover {
	background-position: 0 -12px;
}
#slide_prev a {
	top: 371px;
	left: 750px;
	z-index: 105;
	background-position: -12px 0px;
}
#slide_prev a:hover {
	background-position: -12px -12px;
}
#slide_pause a {
	top: 371px;
	left: 737px;
	z-index: 10001;
	background-position: -24px 0px;
}
#slide_pause a:hover {
	background-position: -24px -12px;
}
#slide_pause .restart {
	background-position: -36px 0px;
}
#slide_pause .restart:hover {
	background-position: -36px -12px;
}
#banner_wrapper {
	position: relative;
	clear: both;
}
.rss_date {
	color: #999999;
	font-size: 11px;
	padding-bottom: 3px;
	padding-top: 2px;
}
.rss_desc {
	color: #FFFFFF;
	font-size: 11px;
