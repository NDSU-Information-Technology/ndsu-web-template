.navbar {
  position: relative;
  display: flex;
  flex-wrap: wrap;
  flex-direction: row;
  list-style: none;
  align-items: center;
  justify-content: space-between;
  background-color: #fff;
  text-transform: uppercase;
  box-sizing: border-box; }
  .navbar .nav-item {
    flex: 0 1 auto;
    display: table;
    margin: 0.5em 0.5em; }
  .navbar a.nav-link {
    color: #000;
    display: table-cell;
    vertical-align: middle; }
  .navbar a.nav-link:hover, .navbar a.nav-link:focus {
    color: #666666; }
  .navbar.navbar-vertical {
    flex-direction: column;
    align-items: left; }
  .navbar.navbar-left {
    justify-content: flex-start; }
    .navbar.navbar-left .nav-item {
      margin-left: 0;
      margin-right: 1em; }
  .navbar.navbar-center {
    justify-content: center; }
  .navbar.navbar-right {
    justify-content: flex-end; }
    .navbar.navbar-right .nav-item {
      margin-left: 1em;
      margin-right: 0; }
  .navbar .nav-item.secondary {
    position: relative; }
    .navbar .nav-item.secondary .secondary-menu {
      text-transform: none;
      background-color: #fff;
      background: rgba(255, 255, 255, 0.9); }
    .navbar .nav-item.secondary .navbar {
      position: absolute;
      left: 0;
      visibility: hidden; }

.sflgru-banner {
