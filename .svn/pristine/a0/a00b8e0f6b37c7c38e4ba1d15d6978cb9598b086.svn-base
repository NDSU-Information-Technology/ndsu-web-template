@import 'variables';
@import 'mixins';

:root {
	font-size: $font-size-rem-base;
}

.sr-only {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  overflow: hidden;
  clip: rect(0,0,0,0);
  white-space: nowrap;
  clip-path: inset(50%);
  border: 0;
}

.sr-only-focusable {
  &:active,
  &:focus {
    position: static;
    width: auto;
    height: auto;
    overflow: visible;
    clip: auto;
    white-space: normal;
    clip-path: none;
  }
}

.sr-navigation {
	position: absolute;
}


a {
	@include linkColor();
	@include linkUnderline();
}

.text-left{
	text-align: left;
}
.text-center{
	text-align: center;
}
.text-right{
	text-align: right;
}
.text-justify{
	text-align: justify;
}

.font-weight-bold{
	font-weight: 600;
}
.font-weight-normal {
	font-weight: normal;
}
.font-size-xs{
	font-size: 0.5rem;
}
.font-size-sm{
	font-size: 0.625rem;
}
.font-size-md{
	font-size: 0.75rem;
}
.font-size-lg{
	font-size: 0.875rem;
}


ul.remove-list-style{
	list-style: none;
}
ul.inline-list{
	list-style: none;
	li {
		margin: 0;
		display: inline-block;
	}
}

.full-overlay{
	display: none;
	content: "";
	position: fixed;
	top: 0; bottom: 0; right: 0; left: 0;
	background-color: rgba($black, 0.75);
	// Default Navbar z-index is 1001
	// Mobile Navbar z-index is 2000
	z-index: 1500;
}

body {
	font-family: $sans-serif;
	font-size: 0.75rem;
	line-height: 1;
	min-height: 100vh;
}

header {
	.green-bar{
		background-color:$green;
		background-image: url(https://static.ndsu.nodak.edu/templates/images/bg_header.jpg);
		background-repeat: no-repeat;
		background-size: auto 100%;
		background-position: top left;
	}

	@include media-xl {

		h2.page-title{
			font-size: 2.5em;
			font-weight: 100;
			padding-top: 0.75em;
			padding-bottom: 0.25em;
		}
	}
	@include media-sm() {
		.page-title-wrapper {
			background-color: $gray7;
			color: $white;
			padding: 0;
			border-bottom: 1px solid $black;
		}
		h2.page-title {
			font-size: 1.25em;
			font-weight: bold;
			padding: 0.75em 1.25em;
		}
	}
}

footer {
	margin-top: 3rem;
	padding: 3em;
	display: flex;
	justify-content: space-between;

	font-size: 1.5em;
	line-height: 1.25em;
	address{
		font-style: normal;
		margin-top: 2em;
	}
	h5, h6 {
		text-transform: uppercase;
	}
	h5 {
		font-size: 1.125em;
		padding: 0 0.75em 0.5em 0.75em;
	}
	.footer-address {
		flex-grow: 1;
		text-align: center;
		align-self: center;
	}
	.footer-links {
		flex-basis: 275px;
	}

	.footer-links .navbar {
		.nav-link {
			padding: 0.5em 1em;
			font-weight: normal;
			text-transform: none;
		}
	}
	.navbar.social-media {
		.nav-link {
			height: 2em;
		}
		.nav-item + .nav-item {
			margin-left: 1em;
		}
	}

	.nav-box {
		margin-bottom: 2em;
	}


	.nav-box.clear-box {
		h5 {
			padding-left: 0;
		}
		.navbar .nav-link {
			padding-left: 0;
			@include linkUnderline(2);
		}
	}

	.nav-box.white-box {
		background-color: $white;
		padding: 2em 0.5em 1em 0.5em;
		.nav-item {
			border-top: 1px solid $black;
		}
		h5 {
			padding-left: 0.25em;
		}
		.navbar .nav-link {
			padding-left: 0.5em;
			@include linkColor($black, $black);
			@include linkUnderline(0);
		}
	}
	.nav-box .navbar-vertical.social-media {
		.social-media-icon {
			height: auto;
			margin: 0.25em 0;
			text-indent: 2em;
		}
		.nav-item + .nav-item {
			margin-left: 0;
		}
		.nav-link {
			@include linkUnderline(-1);
		}
	}

	&.footer-dark,
	&.footer-green {
		color: $white;
		h5, h6 {
			color: $yellow;
		}
		a {
			@include linkColor($white, $yellow);
		}
	}
	&.footer-dark {
		background-color: $gray1;
	}
	&.footer-green {
		background-color: $green;
		background: $gradient-green;
	}

	&.footer-yellow {
		background-color: $yellow;
	}

	@include media-sm {
		flex-direction: column;
		section {
			margin-bottom: 3em;
		}

		.footer-links{
			flex-basis: auto;
		}
	}
}


a.social-media-icon {
	position: relative;
}
a.social-media-icon {
	&::before {
		content: "";
		display: block;
		position: absolute;
		top:0;right:0;bottom:0;left: 0;

		background-position: center left;
		background-repeat: no-repeat;
		background-size: contain;
	}
	&:hover::before,
	&:focus::before {
		opacity: 0.75;
	}

	&.facebook::before {
		background-image: url('data:image/png;base64,');
	}
	&.twitter::before {
		background-image: url('data:image/png;base64,');
	}
	&.instagram::before {
		background-image: url('data:image/png;base64,');
	}
	&.youtube::before {
		background-image: url('data:image/png;base64,');
	}
	&.linkedin::before {
		background-image: url('data:image/png;base64,');
	}
}