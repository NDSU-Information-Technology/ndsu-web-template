/* LINKS */
:root {
  font-size: 16px; }

.sr-only {
  position: absolute;
  width: 1px;
  height: 1px;
  padding: 0;
  overflow: hidden;
  clip: rect(0, 0, 0, 0);
  white-space: nowrap;
  -webkit-clip-path: inset(50%);
          clip-path: inset(50%);
  border: 0; }

.sr-only-focusable:active, .sr-only-focusable:focus {
  position: static;
  width: auto;
  height: auto;
  overflow: visible;
  clip: auto;
  white-space: normal;
  -webkit-clip-path: none;
          clip-path: none; }

.sr-navigation {
  position: absolute; }

a {
  color: #002119; }

a:hover, a:focus, a[aria-expanded=true], a.active {
  color: #999999; }

a {
  text-decoration: none; }

a:hover, a:focus, a[aria-expanded=true], a.active {
  text-decoration: underline; }

.text-left {
  text-align: left; }

.text-center {
  text-align: center; }

.text-right {
  text-align: right; }

.text-justify {
  text-align: justify; }

.font-weight-bold {
  font-weight: 600; }

.font-weight-normal {
  font-weight: normal; }

.font-size-xs {
  font-size: 0.5rem; }

.font-size-sm {
  font-size: 0.625rem; }

.font-size-md {
  font-size: 0.75rem; }

.font-size-lg {
  font-size: 0.875rem; }

ul.remove-list-style {
  list-style: none; }

ul.inline-list {
  list-style: none; }
  ul.inline-list li {
    margin: 0;
    display: inline-block; }

.full-overlay {
  display: none;
  content: "";
  position: fixed;
  top: 0;
  bottom: 0;
  right: 0;
  left: 0;
  background-color: rgba(0, 0, 0, 0.75);
  z-index: 1500; }

body {
  font-family: "Lucida Grande",Arial,Helvetica,sans-serif;
  font-size: 0.75rem;
  line-height: 1;
  min-height: 100vh; }

header .green-bar {
  background-color: #002119;
  background-image: url(https://static.ndsu.nodak.edu/templates/images/bg_header.jpg);
  background-repeat: no-repeat;
  background-size: auto 100%;
  background-position: top left; }

header h2.page-title {
  font-size: 2.5em;
  font-weight: 100;
  padding-top: 0.75em;
  padding-bottom: 0.25em; }

@media (max-width: 767px) {
  header .page-title-wrapper {
    background-color: #121212;
    color: #fff;
    padding: 0;
    border-bottom: 1px solid #000; }
  header h2.page-title {
    font-size: 1.25em;
    font-weight: bold;
    padding: 0.75em 1.25em; } }

main {
  margin-bottom: 3rem; }

footer {
  padding: 3em;
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: justify;
      -ms-flex-pack: justify;
          justify-content: space-between;
  color: #fff;
  font-size: 1.5em;
  line-height: 1.25em; }
  footer a {
    color: #fff; }
  footer a:hover, footer a:focus, footer a[aria-expanded=true], footer a.active {
    color: #ffcc00; }
  footer address {
    font-style: normal; }
  footer h5, footer h6 {
    color: #ffcc00;
    text-transform: uppercase; }
  footer h5 {
    font-size: 1.125em;
    padding: 0 0.75em 0.5em 0.75em; }
  footer .footer-address {
    -webkit-box-flex: 1;
        -ms-flex-positive: 1;
            flex-grow: 1;
    text-align: center; }
  footer .footer-links {
    -ms-flex-item-align: end;
        align-self: flex-end;
    -ms-flex-preferred-size: 20%;
        flex-basis: 20%;
    padding: 1em 0.25em; }
  footer.footer-dark {
    background-color: #030303; }
  footer .footer-links .navbar .nav-link {
    padding: 0.5em 1em;
    font-weight: normal;
    text-transform: none; }
  footer .footer-links .navbar.clear-box .nav-link {
    text-decoration: underline; }
  footer .footer-links .navbar.clear-box .nav-link:hover, footer .footer-links .navbar.clear-box .nav-link:focus, footer .footer-links .navbar.clear-box .nav-link[aria-expanded=true], footer .footer-links .navbar.clear-box .nav-link.active {
    text-decoration: underline; }

.alpha-links li.index a {
  color: #333333;
  text-transform: uppercase; }

.social-media a {
  height: 25px;
  width: 25px;
  background: url(https://static.ndsu.nodak.edu/templates/images/icon_social_media.png) no-repeat top left;
  display: inline-block; }

.social-media a:hover {
  opacity: 0.6; }

.social-media .icon-twitter {
  background-position: -25px 0px; }

.social-media .icon-instagram {
  background-position: -50px 0px; }

.social-media .icon-youtube {
  background-position: -75px 0px; }

.social-media .icon-rss {
  background-position: -100px 0px; }

.social-media .icon-googlemap {
  background-position: -125px 0px; }
