body {
  font-family: "Lucida Grande",Arial,Helvetica,sans-serif;
  font-size: 0.75rem;
  line-height: 1; }

header .top-bar-wrapper {
  background-color: #002119;
  background-image: url(https://static.ndsu.nodak.edu/templates/images/bg_header.jpg);
  background-repeat: no-repeat;
  background-position: top left; }

header .top-bar {
  display: flex;
  flex-direction: row;
  justify-content: space-between; }

header .top-bar .navbar {
  background: none; }

header .top-bar a.nav-link {
  color: #ffcc00; }

header .top-bar a.nav-link:hover, header .top-bar a.nav-link:focus {
  color: #fff; }

header .bottom-bar {
  background-color: #fff; }

footer .footer-nav {
  background-image: url(https://static.ndsu.nodak.edu/templates/images/bg_footernav.png);
  background-repeat: repeat-x;
  background-size: auto 100%; }

footer .footer-nav .navbar {
  background: none; }

footer .footer-nav a.nav-link {
  color: #fff; }

footer .footer-nav a.nav-link:hover, footer .footer-nav a.nav-link:focus {
  color: #ffcc00; }

footer .footer-nav .secondary-menu a.nav-link {
  color: #000; }

footer .footer-nav .secondary-menu a.nav-link:hover, footer .footer-nav .secondary-menu a.nav-link:focus {
  color: #000; }

footer .quick-links {
  padding: 2em 0; }

footer .quick-links ul {
  list-style: none;
  line-height: 1.5em;
  margin: 0 1em; }

footer .quick-links ul li {
  text-indent: -1em;
  margin-left: 1em; }

#homepageCarousel .slide img {
  width: 0; }

#homepageCarousel .slide.current img,
#homepageCarousel .slide.in-transition img {
  width: 100%; }

#alphaLinks li.index a {
  text-transform: uppercase; }

#indexModules .index-module {
  border: 1px solid #404041; }

#indexModules ul li {
  line-height: 1.5em; }

#indexModules h2 {
  padding-bottom: 10px;
  text-transform: uppercase;
  font-weight: 600;
  font-size: 0.75rem;
  color: #ffcc00; }
  #indexModules h2 a {
    color: #ffcc00; }
  #indexModules h2 a:hover, #indexModules h2 a:focus {
    color: #fff; }

#indexModules h6 {
  margin-top: 0.5em;
  font-weight: normal; }

.social-media a {
  height: 25px;
  width: 25px;
  background: url(https://static.ndsu.nodak.edu/templates/images/icon_social_media.png) no-repeat top left;
  display: inline-block; }

.social-media a:hover {
  opacity: 0.6; }

.social-media .icon-twitter {
  background-position: -25px 0px; }

.social-media .icon-instagram {
  background-position: -50px 0px; }

.social-media .icon-youtube {
  background-position: -75px 0px; }

.social-media .icon-rss {
  background-position: -100px 0px; }

.social-media .icon-googlemap {
  background-position: -125px 0px; }

#searchform {
  border: 0; }
  #searchform input {
    font-size: 1.3em;
    height: 2em;
    line-height: 2em;
    border: none;
    margin: 0;
    padding: 0; }
  #searchform input:-ms-input-placeholder {
    color: #222; }
  #searchform input:focus:-ms-input-placeholder {
    color: #6d6e71; }
  #searchform input::-webkit-input-placeholder {
    color: #222; }
  #searchform input:focus::-webkit-input-placeholder {
    color: #6d6e71; }
  #searchform input::-moz-placeholder {
    color: #222; }
  #searchform input:focus::-moz-placeholder {
    color: #6d6e71; }
  #searchform #searchterm {
    background: #6d6e71;
    -moz-border-radius: 0.8em;
    -webkit-border-radius: 0.8em;
    -o-border-radius: 0.8em;
    -ms-border-radius: 0.8em;
    border-radius: 0.8em;
    width: 100%;
    color: #000;
    padding: 0 3.5em 0 0.5em;
    margin: 0;
    box-sizing: border-box;
    -webkit-appearance: none; }
  #searchform #searchterm:focus {
    background-color: #fff;
    outline: none; }
  #searchform #searchsubmit {
    position: relative;
    top: calc(-6px - 1.1em);
    top: -webkit-calc(-6px - 1.1em);
    float: right;
    height: 1.2em;
    padding: 0 0.5em;
    border-left: 2px ridge #333;
    -moz-border-radius: 0;
    -webkit-border-radius: 0;
    -o-border-radius: 0;
    -ms-border-radius: 0;
    border-radius: 0; }
