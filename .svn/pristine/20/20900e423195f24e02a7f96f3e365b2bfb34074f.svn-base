/* LINKS */
.theme-dark {
  background-color: #000;
  color: #aaaaaa; }
  .theme-dark a {
    color: #fff; }
  .theme-dark a:hover, .theme-dark a:focus, .theme-dark a[aria-expanded=true] {
    color: #ffcc00; }

a {
  text-decoration: none; }

a:hover, a:focus, a[aria-expanded=true] {
  text-decoration: underline; }

.theme-gray {
  background-color: #222222;
  color: #fff; }
  .theme-gray a {
    color: #fff; }
  .theme-gray a:hover, .theme-gray a:focus, .theme-gray a[aria-expanded=true] {
    color: #ffcc00; }

a {
  text-decoration: none; }

a:hover, a:focus, a[aria-expanded=true] {
  text-decoration: underline; }
